{
  "name": "react-starter-rspack",
  "version": "0.0.1",
  "description": "quickstart boilerplate for react using typescript/rspack.",
  "keywords": [
    "react",
    "boilerplate",
    "rspack"
  ],
  "homepage": "https://examples-hub.github.io/react-starter-rspack",
  "license": "MIT",
  "author": "jinyaoo <jinyaoo@qq.com>",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "// storybook": "echo 'TODO start-storybook wip'",
    "build": "npm run clean && npm run build:prod && npm run build:server-prod",
    "build11": "npm run build:types && npm run build:js",
    "build:dev": "cross-env REACT_APP_ENV=react rspack build -c ./webpacker/rspack.config.demo.mjs",
    "build:dev-ts": "NODE_OPTIONS=--experimental-transform-types rspack build -c ./webpacker/rspack.config.dev.ts",
    "build:prod": "cross-env REACT_APP_ENV=react NODE_ENV=production rspack build -c ./webpacker/rspack.config.prod.mjs",
    "build:prod-esm": "cross-env REACT_APP_ENV=react NODE_ENV=production rspack build -c ./webpacker/rspack.config.prod.esm.mjs",
    "build:server-dev": "npx tsc --skipLibCheck --target ESNext --module ESNext --moduleResolution node --esModuleInterop --outDir target ./server/app.ts",
    "build:server-prod": "npm run build:server-dev && mv target/app.js target/server.mjs",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf build dist target",
    "demo": "cross-env REACT_APP_ENV=reacthot rspack serve -c ./webpacker/rspack.config.demo.mjs",
    "dev": "npm run demo",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,mjs,mts,json}\" --write",
    "format:check": "npm run formatter -- --check",
    "help": "echo 'exec npm start to view the demo'",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "serve": "tsx watch ./server/app.ts",
    "serve:prod": "npm run build && node ./target/server.mjs",
    "start": "npm run demo",
    "test": "cross-env REACT_APP_ENV=react jest --config ./jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": " npm run lint && npm run format"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@rspack/cli": "^1.3.10",
    "@rspack/core": "^1.3.10",
    "@rspack/plugin-react-refresh": "^1.4.3",
    "@swc/helpers": "^0.5.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "core-js": "^3.41.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "global-jsdom": "^26.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "oxlint": "^0.16.5",
    "prettier": "^3.5.3",
    "react-refresh": "^0.17.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "webpack-merge": "^6.0.1"
  }
}
